          section.quiz
            img(src="img/quiz.jpg").quiz__img
          
          section.complex
            .complex__top
              p.common__title Комплексные поставки
              p.complex__subtitle В наличии более 30 00 товаров с доставкой по россии и СНГ
            .complex__features
              .complex__feature
                img(src="img/complex-icon1.png").complex__feature-icon
                .complex__feature-content
                  p.complex__feature-title Экономия времени
                  p.complex__feature-text Не нужно искать несколько поставщиков
              .complex__feature
                img(src="img/complex-icon2.png").complex__feature-icon
                .complex__feature-content
                  p.complex__feature-title Доставка дешевле
                  p.complex__feature-text Все необходимое в одной поставке
              .complex__feature
                img(src="img/complex-icon3.png").complex__feature-icon
                .complex__feature-content
                  p.complex__feature-title Скидки на технику
                  p.complex__feature-text При покупке стеллажных систем
            .complex__calc
              .complex__calc-item
                .complex__calc-titleover
                  a(href="#").complex__calc-title.js-complex-title-shave Штабелеры
                .complex__calc-imgover
                  img(src="img/complex-img1.jpg").complex__calc-img
                .complex__calc-bottom
                  p.complex__calc-cost от 3700 
                    span руб.
                  label(data-value="Штабелеры", data-default="Выбрать", data-checked="Выбрано").complex__calc-checkbox.js-complex-check
                    input(type="checkbox", value="1").comlex__calc-checktrue
              .complex__calc-item
                .complex__calc-titleover
                  a(href="#").complex__calc-title.js-complex-title-shave Гидравлические тележки
                .complex__calc-imgover
                  img(src="img/complex-img2.jpg").complex__calc-img
                .complex__calc-bottom
                  p.complex__calc-cost от 2300 
                    span руб.
                  label(data-value="Гидравлические тележки", data-default="Выбрать", data-checked="Выбрано").complex__calc-checkbox.js-complex-check
                    input(type="checkbox", value="1").comlex__calc-checktrue
