header.header
  .header__top
    .container
      .town Ваш город: 
        a(href="#city", data-fancybox).town__href.js-town Петропавловск-Камчатский
      nav.menu
        ul.menu__list
          li.menu__item
            a(href="#").menu__item-href.active О компании
          li.menu__item
            a(href="#").menu__item-href Производство
          li.menu__item
            a(href="#").menu__item-href Стать дилером
          li.menu__item
            a(href="#").menu__item-href Контакты
  .header__bottom
    .container
      include __header_burger   

      .header__icons
        a(href="#").header__icons-elem.header__icons-elem_compare 3
        a(href="#").header__icons-elem.header__icons-elem_favorite 2
        span.header__icons-elem.header__icons-elem_card.empty 0
      button(type="button").search__open.js-search-icon
      .header__contacts
        p.header__contacts-row 
          span Бесплатно по России
          a(href="tel:+78002340684") 8 (800) 234-06-84
        p.header__contacts-row 
          span Без выходных с 9:00 до 19:00
          a(href="tel:+74957889934") +7 (495) 788-99-34
      a(href="#").header__logo
        img(src="img/logo.png", alt="").header__logo-img
      nav.navcats
        ul.navcats__list
          li.navcats__item
            a(href="#").navcats__item-href.navcats__item-href_cat Каталог
            ul.navcats__droplist
              li.navcats__dropitem
                a(href="#").navcats__dropitem-href Стеллажи
              li.navcats__dropitem
                a(href="#").navcats__dropitem-href Складская техника
                ul.navcats__sublist
                  li.navcats__subitem
                    a(href="#").navcats__subitem-href Распродажа
                  li.navcats__subitem
                    a(href="#").navcats__subitem-href Тележки
                    ul.navcats__deeplist
                      - var menuList = ['Гидравлические тележки', 'Электрические (самоходные) тележки', 'Платформенные тележки', 'Складные тележки', 'Тележки с бортами']
                      each item in menuList
                        li.navcats__deepitem
                          a(href="#").navcats__deepitem-href= item
                      li.navcats__deepitem
                        a(href="#").navcats__deepitem-href.navcats__deepitem-href_toall Смотреть все модели
                  li.navcats__subitem
                    a(href="#").navcats__subitem-href Штаблёры
                    ul.navcats__deeplist
                      - var menuList = ['Гидравлические штабелёры', 'Штабелёры с электроподъёмом', 'Электроштабелёры самоходные', 'Электрические штабелёры', 'Сборщик заказов']
                      each item in menuList
                        li.navcats__deepitem
                          a(href="#").navcats__deepitem-href= item
                  li.navcats__subitem
                    a(href="#").navcats__subitem-href Ричтраки
                  li.navcats__subitem
                    a(href="#").navcats__subitem-href Шкафы на колёсах
                  li.navcats__subitem
                    a(href="#").navcats__subitem-href Контейнеры на колёсах
                  li.navcats__subitem
                    a(href="#").navcats__subitem-href Погрузчики
                    ul.navcats__deeplist
                      - var menuList = ['Складские минипогрузчики (оператор на подножке)', 'Вилочные электрические погрузчики']
                      each item in menuList
                        li.navcats__deepitem
                          a(href="#").navcats__deepitem-href= item
                  li.navcats__subitem
                    a(href="#").navcats__subitem-href Колёса
                    ul.navcats__deeplist
                      - var menuList = ['Колёса и колёсные опоры для тележек', 'Колёса большегрузные для тележек', 'Колёса промышленные']
                      each item in menuList
                        li.navcats__deepitem
                          a(href="#").navcats__deepitem-href= item
                  li.navcats__subitem
                    a(href="#").navcats__subitem-href Колёсные опоры
                    ul.navcats__deeplist
                      - var menuList = ['Колёсные опоры промышленные', 'Колёсные опоры аппаратные', 'Колёсные опоры пневматические', 'Опоры колёсные большегрузные']
                      each item in menuList
                        li.navcats__deepitem
                          a(href="#").navcats__deepitem-href= item
                  li.navcats__subitem
                    a(href="#").navcats__subitem-href Ролики
                    ul.navcats__deeplist
                      - var menuList = ['Колёса и ролики для гидравлических тележек', 'Специальные колёса и ролики для тележек']
                      each item in menuList
                        li.navcats__deepitem
                          a(href="#").navcats__deepitem-href= item
