.h
  &-serv
    &over
      mt 40px
      display flex
      justify-content space-between
      @media $tLandscapeMax
        flex-direction column
    &left
      ib()
      w calc(100% - 482px)
      max-width 820px
      @media $tLandscapeMax
        w 100%
        max-width 100%
    &right
      ib()
      w 422px
      @media $tLandscapeMax
        dn()
  &-cat
    &over
      mb 65px
      display flex
      justify-content space-between
      @media $tLandscapeMax
        flex-direction column
    &left
      ib()
      w calc(100% - 482px)
      max-width 820px
      @media $tLandscapeMax
        w 100%
        max-width 100%
    &right
      ib()
      w 422px
      @media $tLandscapeMax
        w 100%
        mt 30px
  &-card
    &__title
      color $dark
      fontify 18px 500 24px
    &__alarm
      background-color whitesmoke
      padding 35px 40px
      &-title
        color $orange
        fontify 14px 500 24px
      &-text
        color $dark
        fontify 14px 400 24px
  &-seo
    &__bordered
      border 2px solid #ececec
      margin 30px 0 35px
      padding 45px 50px 50px
      display flex
      @media $tLandscapeMax
        flex-direction column
      @media $mPortraitMax
        padding 25px 30px 30px
    &__left
      ib()
      w calc(100% - 586px)
      @media $tLandscapeMax
        w 100%
        max-width 100%
    &__right
      ib()
      w 521px
      ml 65px
      mt 6px
      .video__elem
        w 100%
        h 270px
        mb 0
      .video__text
        mb 0
      @media $tLandscapeMax
        ml 0
        mt 35px
      @media $mLandscape
        w 100%
  &-contacts
    border 2px solid #ececec
    mt 70px
    padding 33px 48px 50px 48px
    display flex
    @media $tLandscapeMax
      flex-wrap wrap
    @media $mPortraitMax
      padding 13px 28px 30px 28px
    &__title
      color $dark
      fontify 18px 500 24px
      w 270px
      pr 80px
      @media $tLandscapeMax
        w 100%
        mb 20px
        pr 0
    &__text
      margin 10px 0
      color $dark
      fontify 14px 400 22px
      max-width 267px
      &:first-child
        mt 0
      &:last-child
        mb 0
      @media $tPortraitMax
        max-width none
    &__col
      w 340px
      pr 20px
      &:last-child
        w 280px
        @media $tLandscapeMax
          w 30%
        @media $tPortraitMax
          w 100%
      @media $tLandscapeMax
        w 35%
        pr 0
      @media $tPortraitMax
        w 100%
        & + &
          mt 20px
    &__tel
      color $dark
      fontify 18px 500 24px
    &__socials
      display flex
      @media $tLandscapeMax
        flex-wrap wrap
    &__social
      border-radius 50%
      size 45px
      border 2px solid black
      position relative
      transition .5s background
      @media $mPortraitMax
        size 40px
      &:before
        content ""
        position absolute
        transition .5s filter
      &_viber:before
        size 18px 20px
        l 11px
        t 11px
        background url("data:image/png;base64,") no-repeat center
        @media $mPortraitMax
          l 8px
          t 8px
      &_whatsapp:before
        size 19px
        t 11px
        l 11px
        background url("data:image/png;base64,") no-repeat center
        @media $mPortraitMax
          l 8px
          t 8px
      &_vk:before
        size 19px 11px
        l 11px
        t 16px
        background url("data:image/png;base64,") no-repeat center
        @media $mPortraitMax
          l 8px
          t 13px
      &_fb:before
        size 9px 17px
        l 16px
        t 12px
        background url("data:image/png;base64,") no-repeat center
        @media $mPortraitMax
          l 13px
          t 9px
      &:hover
        background black
        &:before
          filter invert(100%)
      & + &
        ml 20px
        @media $tLandscapeMax
          ml 10px