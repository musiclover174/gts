.calc
  pos relative
  z-index 1
  pt 33px
  mb 66px
  &:not(&_inner)
    text-align center
    pb 41px
  &:not(&_inner):not(&_small):before
    content ""
    w calc(100vw + 20px)
    h 100%
    pos absolute
    z-index -1
    t 0
    l 50%
    transform translateX(-50%)
    bgc $orange
  &_small
    mt 70px
    mb -40px
    pt 41px
    .calc__open
      mt 10px
    &:before
      content ""
      w 100%
      h 100%
      pos absolute
      z-index -1
      bgc $orange
      t 0
      l 0
    & + .calc-hidden.open
      mt 80px
      mb -40px
  &__cattile
    max-width 950px
    mb 35px
  &__title
    color white
    fontify 34px 500 38px
    mb 6px
    @media $tLandscapeMax
      fontify 24px 400 34px
    @media $mLandscapeMax
      fontify 22px 400 28px
  &__subtitle
    color white
    fontify 20px 300 24px
    mb 25px
    @media $mLandscapeMax
      fontify 16px 300 24px
  &__open
    bgc white
    pos relative
    color $orange
    letter-spacing .08em
    transition .5s opacity
    &:before
      content attr(data-default)
    &.open:before
      content attr(data-open)
    &:after
      content ""
      display inline-block
      vertical-align middle
      ml 13px
      mt -2px
      size 16px 10px
      background url("data:image/png;base64,") no-repeat center
      transition .5s transform
      will-change transform
    &.open:after
      transform rotate(180deg)
    &:hover
      bgc white
      opacity .8
  &__top
    display flex
    align-items center
    pos relative
    bgc $orange
    padding 20px 30px
    @media $tPortraitMax
      padding 20px
    @media $mLandscape
      flex-direction column
      align-items flex-start
    span
      color white
      fontify 14px 400 23px
      @media $mLandscape
        db()
        mb 10px
      @media $mPortraitMax
        pr 70px
    .choices
      mb 0
      mt 0
      ml 29px
      w 628px
      @media $tLandscapeMax
        w 470px
      @media $tPortraitMax
        w 270px
      @media $mLandscape
        w 100%
        ml 0
      &__inner
        border-color white
      &__list--single
        padding 9px 34px 6px 9px
  &__bottom
    display flex
    align-items stretch
    border 2px solid #ececec
    border-top none
    @media $mLandscape
      flex-direction column
  &__info
    w 340px
    bgc #f5f5f5
    display flex
    padding 0 47px
    @media $tLandscapeMax
      padding 0 27px
      w 300px
    @media $tPortraitMax
      w 270px
    @media $mLandscape
      w 100%
      padding 20px
    &-inner
      align-self center
      @media $mLandscape
        w 100%
    &-title
      color $dark
      fontify 14px 500 23px
      mb 2px
    &-cost
      color $orange
      fontify 32px 500 32px
      mb 27px
      span
        display inline-block
        vertical-align bottom
        fontify 22px 500 22px
    &-delete
      cursor pointer
      position absolute
      r 30px
      t 50%
      transform translateY(-50%)
      color white
      fontify 14px 400 16px
      border-bottom 2px solid white
      transition .5s border
      @media $tPortraitMax
        r 20px
      @media $mLandscape
        t 23px
        transform none
      &:hover
        border-color transparent
    &-btn
      pl 31px
      pr 52px
      pos relative
      @media $tPortraitMax
        pl 21px
        pr 42px
      &:before
        content attr(data-default)
      &.added
        bgc black
        text-align center
        pr 31px
        &:before
          content attr(data-added)
        &:after
          dn()
      &:after
        content ""
        pos absolute
        r 30px
        t 18px
        size 12px
        background url("data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAwAAAAMAQMAAABsu86kAAAABGdBTUEAALGPC/xhBQAAACBjSFJNAAB6JgAAgIQAAPoAAACA6AAAdTAAAOpgAAA6mAAAF3CculE8AAAABlBMVEX///////9VfPVsAAAAAXRSTlMAQObYZgAAAAFiS0dEAIgFHUgAAAAJcEhZcwAACxIAAAsSAdLdfvwAAAATSURBVAjXY2BjYEBG/z+AEKogAEolBBulYreOAAAAAElFTkSuQmCC") no-repeat center
        @media $tPortraitMax
          t 13px
          r 20px
  &__main
    w calc(100% - 340px)
    display flex
    @media $tLandscapeMax
      w calc(100% - 300px)
    @media $tPortraitMax
      w calc(100% - 270px)
    @media $mLandscape
      w 100%
  &__imgover
    w 320px
    justify-content center
    display flex
    padding 20px 0
    @media $tLandscapeMax
      w 260px
    @media $tPortraitMax
      dn()
  &__img
    align-self center
  &__selectors
    w calc(100% - 320px)
    padding 20px 0
    display flex
    @media $tLandscapeMax
      w calc(100% - 260px)
    @media $tPortraitMax
      w 100%
      padding 20px
    &-inner
      align-self center
      w 100%
  &__selector
    width 280px
    ib()
    mb 10px
    font-size 0
    @media $desktopMax
      w 336px
    @media $mPortraitMax
      w 100%
    &_long
      width 336px
      @media $mPortraitMax
        w 100%
      & ^[-1]-sel
        w 186px
        @media $mPortraitMax
          w 100%
    &-label
      ib()
      w 150px
      color $dark
      fontify 14px 400 36px
      @media $mPortraitMax
        w 100%
    &-sel
      ib()
      w 92px
      @media $desktopMax
        w 186px
      @media $mPortraitMax
        w 100%
      .choices
        mb 0
        mt 0
        ml 0
        w 100%
        &__inner
          border-color #b6b6b6
        &__list--single
          padding 9px 34px 6px 9px
  &__result
    mt 54px
    &-title
      color $dark
      fontify 30px 400 36px
      text-align center
      mb 10px
    &-table
      width 100%
      border 1px solid #f5f5f5
    &-buy
      float right
      pos relative
      size 17px
      mt 2px
      background url("data:image/png;base64,") no-repeat center
      transition .5s opacity
      @media $desktopMax
        float none
        db()
      &:hover
        opacity .8
      &.added
        background url("data:image/png;base64,") no-repeat center
    &-bottom
      display flex
      justify-content flex-end
      mt 5px
      @media $mLandscape
        flex-direction column
    &-calced
      display flex
      color $dark
      fontify 18px 500 48px
      text-transform uppercase
      letter-spacing .1em
      mr 27px
      @media $mLandscape
        mr 0
        mb 20px
      > span
        pos relative
        t 5px
        @media $mPortraitMax
          t 2px
      &-cost
        color $orange
        fontify 38px 500 43px
        text-transform none
        letter-spacing 0
        ml 10px
        @media $mPortraitMax
          fontify 28px 500 43px
        span
          display inline-block
          vertical-align bottom
          fontify 23px 500 36px
          pl 10px
  &-hidden
    dn()
    &.open
      db()
      mb 60px
      
.prcalc
  pos relative
  bgc #f6f6f6
  padding 67px 80px 70px 338px
  mb 30px
  @media $tLandscapeMax
    padding 67px 80px 70px
  @media $tPortraitMax
    padding 27px 40px 30px
  @media $mPortraitMax
    padding 27px 0 35px
    bgc transparent
    pos relative
  &:after
    content ""
    pos absolute
    z-index -1
    @media $mPortraitMax
      bgc #f6f6f6
      h 100%
      w calc(100vw + 17px)
      t 0
      l 50%
      transform translateX(-50%)
  &:before
    content ""
    pos absolute
    l 86px
    t 80px
    w 201px
    h 242px
    background url("data:image/png;base64,") no-repeat center
    @media $tLandscapeMax
      dn()
  .common__input
    bgc white
    border 2px solid #b6b6b6
    h 36px
    w calc(100% - 150px)
    padding 0 11px
    @media $mPortraitMax
      w 100%
  .common__col
    display flex
    @media $tPortrait
      &:last-child
        & ^[0]__label
          w 107px
        .common__input,
        .choices
          w calc(100% - 107px)
    @media $mPortraitMax
      flex-direction column
  &__form
    pt 16px
  &__label
    w 150px
    pr 10px
    color $dark
    fontify 14px 400 23px
    pt 6px
    @media $mPortraitMax
      pt 0
      w 100%
      mb 2px
    sup
      font-size 10px
      pos relative
      t -5px
  .choices
    ml 0
    mt 0
    w calc(100% - 150px)
    @media $mPortraitMax
      w 100%
  .common__row:last-child
    align-items flex-start
    @media $mLandscapeMax
      flex-direction column-reverse
  .common__row + .common__row
    mt 30px
  &__filelabel
    color alpha($dark, .5)
    fontify 12px 400 15px
    pl 35px
    mt 10px
    @media $mPortraitMax
      pl 0
  .common__filename
    font-weight 500
    l 35px
    @media $mPortraitMax
      font-weight 400
    &:before
      w 21px
      h 10px
      l -30px
      background url("data:image/png;base64,") no-repeat center
  &__question
    mt -22px
    @media $mLandscapeMax
      mt 0
      mb 15px
    &-title
      color $dark
      fontify 18px 500 30px
    &-phone
      pos relative
      color $orange
      fontify 24px 500 36px
      text-align right
      @media $mLandscapeMax
        text-align left
      &:before
        content ""
        pos absolute
        t 10px
        l -9px
        w 17px
        h 18px
        background url("data:image/png;base64,") no-repeat center
        @media $mLandscapeMax
          dn()
  &__sbm
    @media $mLandscapeMax
      w 100%
.pbsl
  &__title
    ib()
    mr 38px
    pos relative
    color white
    fontify 44px 400 65px
    @media $desktopMax
      fontify 34px 400 65px
    @media $mLandscapeMax
      fontify 22px 400 28px
      db()
      mr 0
      padding 0 20px
      mb 10px
    &:before
      content ""
      ib()
      mr 26px
      size 65px
      background url("data:image/png;base64,") no-repeat center
      @media $mLandscapeMax
        dn()
