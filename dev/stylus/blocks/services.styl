.services
  mb 69px
  @media $tPortraitMax
    mb 39px
  &__list
    display flex
    align-items stretch
    justify-content space-between
    pt 5px
    @media $tPortraitMax
      flex-wrap wrap
      justify-content space-around
    @media $mLandscape
      justify-content flex-start
  &__item
    width calc(20% - 24px)
    text-align center
    @media $desktopMax
      width 20%
    @media $tPortraitMax
      width calc(100% / 3 - 20px)
      mb 30px
    @media $mLandscape
      width 50%
    @media $mPortraitMax
      width 100%
      mb 0
    & + &
      ml 30px
      @media $desktopMax
        ml 0
      @media $tPortraitMax
        ml 30px
      @media $mLandscape
        ml 0
    &-href
      padding 39px 38px 32px
      @media $tLandscapeMax
        padding 29px 28px 22px
      db()
      h 100%
      transition .5s background
      &:hover
        bgc $orange
    &-title
      color $dark
      fontify 16px 500 23px
      @media $tLandscapeMax
        fontify 14px 500 21px
      border-bottom 2px solid $orange
      transition .5s border, .5s color
      span
        pos relative
        top 2px
      ^[-1]:hover &
        border-color transparent
        color white
    &-imgover
      h 57px
      line-height 57px
      mb 31px
      transition .5s filter
      will-change filter
      ^[-1]:hover &
        filter: brightness(0) invert(1);
    &-icon
      display inline-block
      vertical-align middle
      max-height 100%
  &__href
    pos relative
    l 37px
    color $dark
    fontify 14px 500 22px
    border-bottom 2px solid $orange
    transition .5s border, .5s color
    &.active,
    &:hover
      border-color transparent
      color $orange
    &:before
      content ""
      pos absolute
      l -37px
      t 0
      transition .5s background
    &_repairs
      &:before
        l -33px
        size 21px 22px
        background url("data:image/png;base64,") no-repeat center
      &.active:before,
      &:hover:before
        background url("data:image/png;base64,") no-repeat center
    &_relocation
      &:before
        size 28px 22px
        background url("data:image/png;base64,") no-repeat center
      &.active:before,
      &:hover:before
        background url("data:image/png;base64,") no-repeat center
    &_delivery
      &:before
        size 28px 20px
        background url("data:image/png;base64,") no-repeat center
      &.active:before,
      &:hover:before
        background url("data:image/png;base64,") no-repeat center
    &_install
      &:before
        size 25px
        background url("data:image/png;base64,") no-repeat center
      &.active:before,
      &:hover:before
        background url("data:image/png;base64,") no-repeat center
    &_chefinstall
      &:before
        size 20px 22px
        background url("data:image/png;base64,") no-repeat center
      &.active:before,
      &:hover:before
        background url("data:image/png;base64,") no-repeat center
    &list
      display flex
      flex-wrap wrap
      ml -29px
      w calc(100% + 29px)
      mb 25px
    &item
      ml 29px
      pr 37px
      mb 20px
      @media $desktopMax
        ml 19px
  &__content
    display flex
    justify-content space-between
    @media $tPortraitMax
      flex-direction column
  &__main
    w calc(100% - 454px)
    @media $desktopMax
      w calc(100% - 400px)
    @media $tLandscapeMax
      w calc(100% - 340px)
    @media $tPortraitMax
      w 100%
  &__variants
    &-href
      color $dark
      fontify 18px 500 24px
      border-bottom 2px solid $orange
      transition .5s border
      &:hover
        border-color transparent
      & + div[class\^="table-scroller"]
        mt 10px
      @media $mPortraitMax
        fontify 16px 500 22px
    &:last-child
      mb -23px
    &-checkover
      float right
      db()
      pos relative
      size 22px
      border 2px solid #b6b6b6
      bgc white
      cursor pointer
      &:before
        content ""
        pos absolute
        l 1px
        t 2px
        opacity 0
        size 16px 13px
        background url("data:image/png;base64,") no-repeat center
        transition .5s opacity
      &.clicked:before
        opacity 1
    &-checkbox
      pos absolute
      w 0 
      h 0
      border 0
      opacity 0
    &-table
      w 100%
      border 1px solid whitesmoke
      @media $mPortraitMax
        w auto
        min-width 480px
      tr
        transition .5s background
      tr.choosed,
      tr:hover
        bgc #fcede3
    &-thname
      w calc(100% - 235px)
      @media $mPortraitMax
        w auto
    &-thcost
      w 235px
      @media $mPortraitMax
        w auto
  &__cost
    w 402px
    pos relative
    @media $desktopMax
      w 350px
    @media $tLandscapeMax
      w 300px
    @media $tPortraitMax
      w 100%
      mt 30px
    &-sticky
      position absolute
      l 0
      w 100%
      t 0
      bgc whitesmoke
      padding 45px 50px
      @media $desktopMax
        padding 35px 40px
      @media $tPortraitMax
        position static !important
        width 100% !important
        transform none !important
    &-title
      color $dark
      fontify 30px 400 36px
      mb 23px
      @media $tLandscapeMax
        fontify 24px 400 28px
    &-digits
      color $orange
      fontify 22px 500 26px
      mb 37px
    &-tocart
      w 100%
      padding 0
      text-align center
      mb 25px
    &-hrefrow
      pl 35px
      & + &
        mt 15px
    &-href
      pos relative
      color $dark
      fontify 14px 500 22px
      border-bottom 2px solid $orange
      transition .5s border
      &:hover
        border-color transparent
      & + &
        ml 55px
      &:before
        content ""
        pos absolute
      &_mail:before
        t 4px
        l -34px
        size 24px 13px
        background url("data:image/png;base64,") no-repeat center
      &_print:before
        t 2px
        l -32px
        size 17px 16px
        background url("data:image/png;base64,") no-repeat center
  &__device
    mt 25px
    &-title
      color $dark
      fontify 30px 400 36px
      border-bottom 2px solid $orange
      transition .5s border
      span
        pos relative
        t 5px
      &:hover
        border-color transparent
    &-text
      mt 32px
      mb 34px
    &-mainover
      w 100%
      pt 57%
      overflow hidden
      pos relative
      cursor pointer
      &:before
        content ""
        pos absolute
        l 0 
        t 0
        w 100%
        h 100%
        bgc #1a1a18
        z-index 1
        opacity 0
        transition .5s opacity
      &:after
        content ""
        pos absolute
        z-index 2
        l 50%
        t 50%
        size 30px 32px
        mt -15px
        ml -16px
        background url("data:image/png;base64,") no-repeat center
        opacity 0
        transition .5s opacity
      &:hover:before
        opacity .6
      &:hover:after
        opacity 1
    &-mainimg
      imgCentering()
    &-glightbox
      visibility hidden
    &-car
      pos relative
      margin 18px 33px 50px
      &-over
        pos relative
        .swiper-button-prev
          l -17px
          outline none
          bgc transparent
        .swiper-button-next
          r -17px
          outline none
          bgc transparent
    &-carelem
      w 100%
      pt 100%
      cursor pointer
      border 2px solid #ececec
      transition .5s border
      &.active,
      &:hover
        border-color $orange
    &-img
      imgCenteringMax()