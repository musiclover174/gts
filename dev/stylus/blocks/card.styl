.card
  &__list
    display flex
    flex-wrap wrap
    ml -32px
    mb -30px
    w calc(100% + 32px)
  &__elem
    pos relative
    w calc(100% / 3 - 32px)
    ml 32px
    mb 30px
    border 2px solid #ececec
    padding 21px 29px 30px
    transition .5s border
    &:hover
      border-color $orange
    @media $tLandscapeMax
      w calc(50% - 32px)
    @media $mLandscape
      w calc(100% - 32px)
    &-hrefover
      mb 10px
      max-width 295px
      pr 35px
      mb 20px
      height 50px
      overflow hidden
      text-overflow ellipsis
      @media $mLandscape
        max-width 100%
        overflow visible
        height auto
    &-href
      color $dark
      fontify 16px 500 24px
      border-bottom 2px solid $orange
      transition .5s border
      &:hover
        border-color transparent
    &-checkfake
      db()
      pos relative
      size 28px
      border 2px solid #b6b6b6
      cursor pointer
      &:before
        content ""
        pos absolute
        l 4px
        t 4px
        opacity 0
        size 16px 13px
        background url("data:image/png;base64,") no-repeat center
        transition .5s opacity
      &.clicked:before
        opacity 1
    &-checktrue
      pos absolute
      w 0 
      h 0
      border 0
      opacity 0
    &-checkbox
      t 23px
      r 22px
      pos absolute
      size 28px
      & + ^[-1]-icons
        t 71px
    &-icons
      t 28px
      r 27px
      pos absolute
      opacity 0
      transition .5s opacity
      ^[-1]:hover &
        opacity 1
    &-tofavorites
      db()
      border 0
      cursor pointer
      size 18px
      outline none
      background url("data:image/png;base64,") no-repeat center
      transition .5s background
      &.added,
      &:hover 
        background url("data:image/png;base64,") no-repeat center
    &-tocompare
      db()
      mt 15px
      border 0
      cursor pointer
      size 18px 14px
      outline none
      background url("data:image/png;base64,") no-repeat center
      transition .5s background
      &.added,      
      &:hover
        background url("data:image/png;base64,") no-repeat center
      &:first-child
        mt 0
    &-info
      display flex
      justify-content space-between
    &-params
      w 133px
      @media $mPortraitMedium
        w 100%
      &-row
        max-width 110px
        @media $mPortraitMedium
          max-width 100%
        & + &
          mt 11px
      &-title
        color alpha($dark, .5)
        fontify 11px 400 15px
      &-val
        color $orange
        fontify 16px 400 24px
    &-imgover
      w calc(100% - 133px)
      display flex
      justify-content center
      pos relative
      @media $mPortraitMedium
        dn()
    &-img
      align-self center
      max-width 90%
      max-height 100%
    &-modified
      pos absolute
      b 0
      r 0
      &_new
        size 56px
        bgc #3eaa14
        border-radius 50%
        color white
        text-align center
        fontify 14px 500 56px
        text-transform uppercase
        &:before
          content "NEW"
      &_actions
        size 63px 23px
        bgc #ff1b41
        border-radius 12px
        color white
        text-align center
        fontify 12px 500 23px
        text-transform uppercase
        letter-spacing .04em
        &:before
          content "Акция"
      &_hit
        size 56px
        background url("data:image/png;base64,") no-repeat center
        border-radius 50%
        color white
        text-align center
        fontify 14px 500 56px
        text-transform uppercase
        &:before
          content "ХИТ"
    &-price
      border-top 1px solid #ececec
      pt 30px
      mt 17px
      pos relative
      display flex
      justify-content space-between
      @media $mPortraitMedium
        flex-direction column
    &-cost
      color $orange
      fontify 22px 400 22px
      pt 12px
      sub
        font-size 15px
      @media $tPortraitMax
        pt 4px
    &-prevcost
      pos absolute
      color #a5a5a5
      fontify 14px 400 14px
      t 24px
      l 0
      text-decoration line-through
      @media $tPortraitMax
        t 18px
    &-tocart
      pos relative
      pl 58px
      pr 28px
      font-family FiraSans
      &:before
        content attr(data-default)
      &:after
        content ""
        size 17px
        pos absolute
        l 31px
        t 16px
        background url("data:image/png;base64,") no-repeat center
        @media $tPortraitMax
          l 16px
          t 11px
      &.added
        background $dark
        &:before
          content attr(data-added)
      @media $tPortraitMax
        pl 41px
        pr 17px
      @media $mPortraitMedium
        mt 15px
        mt 15px
  &__main
    display flex
    align-items flex-start
    justify-content space-between
    @media $mLandscape
      flex-direction column
    &-about
      w calc(100% - 454px)
      pr 32px
      @media $desktopMax
        w calc(100% - 380px)
        pr 30px
      @media $tPortraitMax
        w calc(100% - 300px)
      @media $mLandscape
        w 100%
        pr 0
    &-top
      display flex
      @media $tLandscapeMax
        flex-direction column
    &-gallery
      w 422px
      mr 32px
      @media $tLandscapeMax
        w 100%
    &-mainover
      w 100%
      pt 112%
      overflow hidden
      pos relative
      cursor pointer
      border 2px solid #ececec
      &:before
        content ""
        pos absolute
        l 0 
        t 0
        w 100%
        h 100%
        bgc #1a1a18
        z-index 1
        opacity 0
        transition .5s opacity
      &:after
        content ""
        pos absolute
        z-index 2
        l 50%
        t 50%
        size 30px 32px
        mt -15px
        ml -16px
        background url("data:image/png;base64,") no-repeat center
        opacity 0
        transition .5s opacity
      &:hover:before
        opacity .6
      &:hover:after
        opacity 1
    &-mainimg
      imgCentering()
    &-glightbox
      visibility hidden
    &-car
      pos relative
      margin 18px 33px 50px
      &-over
        pos relative
        .swiper-button-prev
          l -17px
          outline none
          bgc transparent
        .swiper-button-next
          r -17px
          outline none
          bgc transparent
    &-carelem
      w 100%
      pt 100%
      pos relative
      cursor pointer
      border 2px solid #ececec
      transition .5s border
      &_video:before
        content ""
        size 30px
        pos absolute
        z-index 2
        l 50%
        t 50%
        mt -15px
        ml -15px
        background url("data:image/png;base64,") no-repeat center
      &.active,
      &:hover
        border-color $orange
    &-img
      imgCenteringMax()
  &__info
    w 414px
    @media $desktopMax
      w 364px
    @media $tPortraitMax
      w 300px
    @media $mLandscape
      w 100%
      height auto !important
    &-sticky
      w 100%
      padding 40px
      bgc whitesmoke
      @media $desktopMax
        padding 30px
      @media $mLandscape
        width 100% !important
        transform none !important
        position static !important
        mt 30px
    &-top
      display flex
      justify-content space-between
      mb 26px
      @media $tPortraitMax
        display block
      @media $mLandscape
        display flex
      @media $mPortraitMax
        display block
    &-art
      bgc $orange
      border-radius 15px
      color white
      fontify 14px 500 30px
      padding 0 15px
      @media $tPortraitMax
        ib()
        mb 15px
    &-buttons
      display flex
      align-items center
    &-tofavorites
      border 0
      cursor pointer
      size 18px
      outline none
      ml 20px
      background url("data:image/png;base64,") no-repeat center
      transition .5s background
      &.added,
      &:hover 
        background url("data:image/png;base64,") no-repeat center
    &-toprint
      border 0
      cursor pointer
      size 17px 16px
      outline none
      ml 20px
      background url("data:image/png;base64,") no-repeat center
      transition .5s background
      &:hover 
        background url("data:image/png;base64,") no-repeat center
    &-tocomment
      border 0
      cursor pointer
      size 18px 16px
      outline none
      ml 20px
      background url("data:image/png;base64,") no-repeat center
      &:hover 
        background url("data:image/png;base64,") no-repeat center
    &-tocompare
      border 0
      cursor pointer
      size 18px 14px
      outline none
      background url("data:image/png;base64,") no-repeat center
      transition .5s background
      &.added,      
      &:hover
        background url("data:image/png;base64,") no-repeat center
    &-existence
      color $orange
      fontify 17px 500 22px
      text-transform uppercase
      letter-spacing 0.04em
      mb 20px
    &-row
      display flex
      mb 18px
      @media $tPortraitMax
        flex-direction column
      @media $mLandscape
        flex-direction row
      @media $mPortraitMax
        flex-direction column
      &_count
        margin 36px 0 25px
        @media $tPortraitMax
          margin 15px 0
    &-label
      w 115px
      pr 15px
      color $dark
      fontify 14px 400 16px
      pt 3px
      @media $tPortraitMax
        w 100%
        pt 0
        pr 0
        mb 5px
    &-value
      w calc(100% - 115px)
      @media $tPortraitMax
        w 100%
    &-cost
      color $orange
      fontify 25px 500 26px
      span
        font-size 18px
      &_old
        color $dark
        font-family FiraSans
        fontify 14px 400 22px
        text-decoration line-through
      &-time
        ib()
        mt 3px
        background-color #e6e6e6
        padding 0 5px
        color #a5a5a5
        font-family FiraSans
        fontify 12px 400 19px
    &-delivery
      color $dark
      fontify 14px 500 22px
      span
        color #a5a5a5
        font-family FiraSans
        fontify 12px 400 22px
    &-bottom
      display flex
      flex-wrap wrap
      border-top 1px solid #dddddd
      mt 27px
      pt 24px
      @media $tPortraitMax
        flex-direction column
      & ^[0]__elem-tocart
        mt 18px
        w 100%
        &:after
          l 115px
          @media $desktopMax
            l 100px
          @media $tPortraitMax
            l 70px
          @media $mLandscape
            dn()
    &-calc
      w 100px
      ml 20px
      float right
      mt 1px
      @media $tPortraitMax
        ml 0
        mt 10px
        w 100%
      &-href
        color $dark
        fontify 14px 500 22px
        border-bottom 2px solid $orange
        transition .5s border
        &:hover
          border-color transparent
    &-maxval
      pos relative
      ib()
      mt -5px
      .common__input
        bgc white
        border 2px solid #b6b6b6
        w 92px
        h 36px
        text-align center
      &-plus
        pos absolute
        t 12px
        l 68px
        cursor pointer
        size 12px
        background url("data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAwAAAAMAgMAAAArG7R0AAAABGdBTUEAALGPC/xhBQAAACBjSFJNAAB6JgAAgIQAAPoAAACA6AAAdTAAAOpgAAA6mAAAF3CculE8AAAACVBMVEXnbxfnbxf///+r1ZcnAAAAAXRSTlMAQObYZgAAAAFiS0dEAmYLfGQAAAAJcEhZcwAACxIAAAsSAdLdfvwAAAAVSURBVAjXY2AQYWBAx6GhoWCMRQ4AQsACx/Vqqw0AAAAASUVORK5CYII=") no-repeat center
        transition .5s opacity
        &:hover
          opacity .7
      &-minus
        pos absolute
        t 12px
        l 12px
        cursor pointer
        size 12px
        background url("data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAwAAAACAQMAAABWsa/UAAAABGdBTUEAALGPC/xhBQAAACBjSFJNAAB6JgAAgIQAAPoAAACA6AAAdTAAAOpgAAA6mAAAF3CculE8AAAABlBMVEXnbxf///94NDtdAAAAAWJLR0QB/wIt3gAAAAlwSFlzAAALEgAACxIB0t1+/AAAAAtJREFUCNdjYAABAAAGAAFm9MlsAAAAAElFTkSuQmCC") no-repeat center
        transition .5s opacity
        &:hover
          opacity .7
    &-msc
      ib()
      mt 6px
      pos relative
      pl 34px
      color $dark
      fontify 12px 400 15px
      &:before
        content ""
        size 20px
        bgc white
        border 2px solid #b6b6b6
        pos absolute
        l 0
        t 4px
        cursor pointer
      &:after
        content ""
        pos absolute
        l 5px
        t 10px
        size 14px 12px
        background url("data:image/png;base64,") no-repeat center
        opacity 0
        cursor pointer
        transition .5s opacity
      &.checked:after
        opacity 1
      input
        pos absolute
        l 0
        t 0
        w 0 
        h 0
        visibility hidden
        border 0
        user-select none
        pointer-events none
      &-town
        color $dark
        fontify 12px 400 15px
        border-bottom 1px dotted $orange
        transition .5s border
        &:hover
          border-color transparent
  &__cost
    w calc(100% - 120px)
    display flex
    flex-direction column
    @media $tPortraitMax
      w 100%
    &-title
      color $dark
      fontify 14px 500 23px
      mb 4px
    &-value
      color $orange
      fontify 32px 500 32px
      span
        font-size 22px
  &__params
    mb 50px
    &-title
      color $dark
      fontify 18px 500 23px
      mb 15px
    &-hidden
      dn()
      &.show
        db()
        mt 20px
    &-row
      display flex
      & + &,
      &:last-child
        mt 20px
    &-label
      color alpha($dark, .5)
      fontify 14px 400 22px
      w 149px
      pr 10px
      @media $desktopMax
        w 135px
      @media $tLandscapeMax
        w 250px
      @media $tPortraitMax
        w 135px
      @media $mLandscape
        w 30%
      @media $mPortraitMax
        w 50%
    &-value
      w calc(100% - 149px)
      color $dark
      fontify 14px 400 22px
      @media $desktopMax
        w calc(100% - 135px)
      @media $tLandscapeMax
        w calc(100% - 250px)
      @media $tPortraitMax
        w calc(100% - 135px)
      @media $mLandscape
        w 70%
      @media $mPortraitMax
        w 50%
    .choices,
    .common__select
      color $dark
      w 86px
      mr 10px
    &-tochars
      pos relative
      color $dark
      fontify 14px 400 16px
      border-bottom 2px dotted $orange
      transition .5s border
      &:hover
        border-color transparent
      &:after
        content attr(data-default)
      &.open:after
        content attr(data-open)
      &:before
        content ""
        pos absolute
        t 3px 
        r -15px
        size 10px 14px
        background url("data:image/png;base64,") no-repeat center
        transition .5s transform
        will-change transform
      &.open:before
        transform rotate(180deg)
  &__feats
    display flex
    bgc $orange
    padding 32px 28px
    mt 75px
    mb 80px
    @media $tLandscapeMax
      padding 32px 0
    @media $tPortraitMax
      flex-direction column
    @media $tPortraitMax
      padding 10px 20px
    &-elem
      w 20%
      color white
      fontify 15px 400 22px
      text-transform uppercase
      letter-spacing .1em
      padding 0 40px
      text-align center
      align-self stretch
      @media $desktopMax
        padding 0 20px
      @media $tLandscapeMax
        fontify 14px 400 22px
        padding 0 10px
      @media $tPortraitMax
        w 100%
        padding 17px 20px
      &-text
        pos relative
        top 50%
        transform translateY(-50%)
        @media $tPortraitMax
          t 0
          transform none
      span
        text-transform none
        letter-spacing 0
        db()
      & + &
        border-left 1px solid #e88842
        @media $tPortraitMax
          border-left none
          border-top 1px solid #e88842
        
  &__delivery
    mt 80px
    &-title
      color $dark
      fontify 30px 400 26px
      mb 32px
      
.swiper-slide .card__elem
  mb 0