.header
  pos fixed
  t 0
  l 0
  r 0
  z-index 10
  box-shadow 0 6px 15px rgba(0, 0, 0, 0.05)
  &__top
    bgc #1a1a18
    @media $mLandscape
      dn()
  &__bottom
    h 86px
    bgc white
    @media $mLandscape
      h 72px
  &__icons
    float right
    mt 34px
    ml 51px
    @media $mLandscape
      dn()
    &-elem
      ib()
      pos relative
      color white
      size 18px
      bgc $orange
      fontify 12px 500 18px
      border-radius 50%
      text-align center
      transition .5s background
      &.empty
        background black
      &:before
        content ""
        pos absolute
        r 22px
        t 1px
      &_card:before
        size 17px
        background url("data:image/png;base64,") no-repeat center
      &_favorite:before
        size 18px
        t 0
        background url("data:image/png;base64,") no-repeat center
      &_compare:before
        size 18px 14px
        t 2px
        background url("data:image/png;base64,") no-repeat center
      & + &
        ml 51px
        @media $desktopMax
          ml 38px
      &:not(.empty):hover
        background alpha($orange, 0.8)
    
  &__contacts
    float right
    text-align right
    mt 19px
    @media $tLandscapeMax
      mt 4px
    @media $tPortraitMax
      dn()
    &-row
      color alpha(#0e0e0e, .8)
      fontify 12px 400 18px
      span
        @media $tLandscapeMax
          db()
      a
        ib bottom
        ml 13px
        @media $desktopMax
          ml 7px
        fontify 16px 700 20px
        color #0e0e0e
        @media $tLandscapeMax
          fontify 14px 700 16px
      & + &
        mt 5px
        @media $tLandscapeMax
          mt 3px
  &__logo
    ib()
    mt 18px
    mr 91px
    @media $desktopMax
      mr 51px
    @media $tPortraitMax
      mr 27px
    @media $mLandscape
      mt 10px
.town
  float right
  color white
  fontify 12px 400 16px
  mt 11px
  pos relative
  &:before
    content ""
    pos absolute
    l -20px
    t 1px
    w 10px
    h 13px
    background url("data:image/png;base64,") no-repeat center
  &__href
    color inherit
    border-bottom 1px dotted alpha(#fff, 0.3)
    transition .5s border
    &:hover
      border-color transparent
.menu
  display flex
  font-size 0
  &__item
    display inline-block
    vertical-align top
    & + &
      ml 28px
      @media $desktopMax
        ml 20px
    &-href
      display inline-block
      vertical-align top
      color white
      fontify 10px 700 36px
      text-transform uppercase
      letter-spacing 0.1em
      border-bottom 2px solid transparent
      transition .5s border, .5s color
      &.active,
      &:hover
        color $orange
        border-color $orange
.search
  pos fixed
  t 0
  l 0
  w 100%
  h 100%
  z-index 11
  text-align center
  overflow auto
  pt 100px
  opacity 0
  visibility hidden
  transition .5s opacity, .5s visibility
  &.open
    opacity 1
    visibility visible
  &__bg
    pos fixed
    t 0
    l 0
    w 100%
    h 100%
    z-index -1
    bgc #222222
  &__form
    w 90%
    max-width 990px
    margin 0 auto
    text-align left
    pos relative
    @media $tLandscapeMax
      max-width 100%
      w 100%
      padding 0 20px
  &__icon
    pos absolute
    r 0 
    t 19px
    z-index -1
    size 21px
    background url("data:image/png;base64,") no-repeat center
    @media $tLandscapeMax
      r 20px
  &__inp
    bgc transparent
    border 0
    border-bottom 1px solid white
    w 100%
    color white
    font-family $os
    fontify 30px 300 60px
  &__close
    ib()
    position absolute
    r 50px
    t 50px
    size 20px
    background url("data:image/png;base64,") no-repeat center
    transition: .5s background;
    @media $tLandscapeMax
      t 20px
      r 20px
    &:hover
      background url("data:image/png;base64,") no-repeat center
  &__title
    color white
    fontify 18px 500 21px
    mb 36px
    mt 50px
    &:first-child
      mt 0
  &__res
    w 100%
    margin 56px 0 44px
    visibility hidden
    opacity 0
    transition 1s visibility, 1s opacity
    &.open
      visibility visible
      opacity 1
    &-list
      display flex
      flex-direction column
      &_products
        w calc(100% + 340px)
        flex-direction row
        justify-content space-between
        @media $lDesktopMax
          w 100%
        @media $mLandscape
          flex-direction column
    &-elem
      db()
      & + &
        mt 14px
    &-href
      ib()
      color white
      fontify 14px 400 22px
      border-bottom 1px solid $orange
      white-space nowrap
      max-width 100%
      text-overflow ellipsis
      overflow hidden
      transition .5s border
      &:hover
        border-color transparent
    &-toall
      ib()
      mt 37px
      color white
      pos relative
      fontify 14px 500 18px
      border-bottom 2px solid $orange
      transition .5s border
      &:hover
        border-color:transparent;
      &:after
        content ""
        pos absolute
        r -33px
        t 1px
        size 24px 16px
        background url("data:image/png;base64,") no-repeat center
  &__product
    display flex
    w calc(100% / 3 - 20px)
    @media $lDesktopMax
      w calc(50% - 10px)
      &:last-child
        dn()
    @media $mLandscape
      w 100%
      & + &
        mt 25px
    &-imgover
      db()
      bgc white
      size 120px
      border 2px solid #ececec
      pos relative
      overflow hidden
      @media $mPortraitMedium
        dn()
    &-content
      w calc(100% - 140px)
      ml 20px
      pt 5px
      max-width 228px
      @media $mPortraitMedium
        max-width 100%
        ml 0
        w 100%
    &-href
      color white
      fontify 14px 500 20px
      border-bottom 1px solid $orange
      transition .5s border
      &:hover
        border-color transparent
    &-img
      imgCentering()
    &:hover &-img
      imgCenteringZoom()
    &-chars
      display flex
      justify-content space-between
      mt 10px
    &-weight
      color white
      fontify 12px 400 20px
      pos relative
      pl 23px
      &:before
        content ""
        pos absolute
        l 0
        t 3px
        size 17px 15px
        background url("data:image/png;base64,") no-repeat center
    &-size
      color white
      fontify 12px 400 20px
      pos relative
      pl 18px
      &:before
        content ""
        pos absolute
        l 0
        t 3px
        size 13px
        background url("data:image/png;base64,") no-repeat center
    &-price
      display flex
      mt 10px
    &-cost
      color #fff
      fontify 22px 500 22px
      sub
        font-size 15px
    &-prevcost
      color #fff
      pt 10px
      pl 24px
      font-family FiraSans
      fontify 14px 400 14px
      text-decoration line-through
  &__open
    float right
    size 17px
    mt 34px
    ml 40px
    cursor pointer
    outline none
    border 0
    background url("data:image/png;base64,") no-repeat center
    transition .5s opacity
    &:hover
      opacity .8
    @media $tLandscapeMax
      ml 34px
    @media $mLandscapeMax
      mt 28px
      ml 0
      mr 13px
.navcats
  ib()
  mt 33px
  @media $mLandscape
    dn()
  &__list
    font-size 0
  &__item
    ib()
    & + &
      ml 85px
      @media $desktopMax
        ml 55px
      @media $tPortraitMax
        ml 25px
    &-href
      pos relative
      color #0e0e0e
      fontify 15px 700 21px
      text-transform uppercase
      letter-spacing .1em
      transition .5s color
      &:before
        content ""
        pos absolute
        r calc(100% + 14px)
        t 50%
        transform translateY(-50%)
        transition .5s background
        @media $desktopMax
          r calc(100% + 8px)
        @media $tPortraitMax
          dn()
      &_stell:before
        size 22px 28px
        background url("data:image/png;base64,") no-repeat center
      &_stell:hover:before
          background url("data:image/png;base64,") no-repeat center
      &_tech:before
        size 23px 33px
        background url("data:image/png;base64,") no-repeat center
      &_tech:hover:before
        background url("data:image/png;base64,") no-repeat center
      &_serv:before
        size 30px 28px
        background url("data:image/png;base64,") no-repeat center
      &_serv:hover:before
        background url("data:image/png;base64,") no-repeat center
      &:hover
        color $orange
        
.burger
  float right
  mt 14px
  mr -14px
  @media $tPortrait
    dn()
  &__shadow
    pos fixed
    w 100%
    h 0
    t 0
    l 0
    bgc #e97e2f
    z-index 2
    transition .5s height
  &__menu
    display flex
    flex-direction column
    flex-wrap wrap
    pos fixed
    z-index 3
    w 100%
    h 100%
    l 0
    t 0
    padding 60px 20px
    text-align center
    overflow auto
    visibility hidden
    opacity 0
    transition .5s opacity, .5s visibility
    @media $tPortrait
      dn()
    &.open
      visibility visible
      opacity 1
    .town
      float none
      fontify 15px 400 18px
      &:before
        dn()
    .menu
      &__list
        mt 30px
      &__item
        db()
        & + .menu__item
          ml 0
          mt 20px
        &-href
          fontify 15px 400 28px
          &.active,
          &:hover
            border-color transparent
            color white
            font-weight 700
    .header__icons
      db()
      float none
      ml 0
      &-elem:before
        filter grayscale(100%) invert(100%)
    .search
      db()
      float none
      ml 0
      w 100%
      h 48px
      &__icon
        position absolute
        right 14px
        t 50%
        mt -8px
      &__input
        w 100%
        l 0
        padding 0 39px 0 19px
        visibility visible
        opacity 1
  &__icon
    outline none
    size 44px
    pos relative
    border 0
    cursor pointer
    z-index 4
    bgc transparent
    &:before,
    span,
    &:after
      size 16px 2px
      content ""
      bgc $orange
    &:before
      pos absolute
      t 14px
      l 14px
      transition .5s transform, .5s background
      will-change transform
    span
      pos absolute
      t 21px
      l 14px
      transition .5s opacity
    &:after
      pos absolute
      b 14px
      l 14px
      transition .5s transform, .5s background
      will-change transform
    &.open
      span
        opacity 0
      &:before
        transform rotate(225deg)
        t 21px
        bgc white
      &:after
        transform rotate(-225deg)
        b 21px
        bgc white
      & + ^[0]__shadow
        h 100%